//- Generates a head for the document.
mixin head(data)
    title=data.title
    script(
        src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"
    )
    link(
        rel="icon" 
        href="/public/icons/icon.ico" 
        type="image/x-icon"
    )
    link(
        rel="stylesheet" 
        href="/public/styles/style.css"
    )
