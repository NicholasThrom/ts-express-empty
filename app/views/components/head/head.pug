//- Generates a head for the document.
mixin head(data)
    title=data.title
    script(
        src="/public/scripts/script.js"
    )
    link(
        rel="icon" 
        href="/public/icons/icon.ico" 
        type="image/x-icon"
    )
    link(
        rel="stylesheet" 
        href="/public/styles/style.css"
    )
