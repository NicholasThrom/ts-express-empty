{
    "extension": [
        ".ts"
    ],
    "exclude": [
        "**/*.d.ts",
        "public/**/*",
        "test/**/*",
        "coverage/**/*",
        "webpack.config.js"
    ],
    "reporter": [
        "html",
        "text"
    ],
    "watermarks": {
      "lines": [80, 100],
      "functions": [80, 100],
      "branches": [80, 100],
      "statements": [80, 100]
    },
    "all": true
}
