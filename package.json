{
    "name": "ts-express-empty",
    "version": "0.0.0",
    "description": "Just an empty ts/express app to save some time setting up.",
    "main": "index.js",
    "scripts": {
        "start": "node ./scripts/start.js",
        "test": "mocha",
        "testing": "mocha -r ts-node/register -require source-map-support/register -w",
        "compile": "tsc && npm run concatenate -s",
        "compiling": "npm run concatenating -s & tsc -w",
        "coverage": "nyc npm test -s",
        "lint": "tslint --project ./tsconfig.json && pug-lint ./",
        "linting": "watch 'npm run lint -s' -d ./",
        "concatenate": "find ./app/public/subscripts -name '*.js' | xargs cat > ./app/public/scripts/script.js",
        "concatenating": "npm run concatenate -s ; watch 'npm run concatenate -s' ./app/public/subscripts/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NicholasThrom/ts-express-empty.git"
    },
    "keywords": [
        "typescript",
        "ts",
        "express",
        "empty"
    ],
    "author": "Nicholas Throm",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NicholasThrom/ts-express-empty/issues"
    },
    "homepage": "https://github.com/NicholasThrom/ts-express-empty#readme",
    "dependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/chai": "^4.1.2",
        "@types/cookie-parser": "^1.4.1",
        "@types/express": "^4.11.0",
        "@types/less-middleware": "^2.0.31",
        "@types/mocha": "^2.2.47",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.3.0",
        "@types/sinon": "^4.1.3",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "cookie-parser": "^1.4.3",
        "express": "^4.16.2",
        "less-middleware": "^2.2.1",
        "mocha": "^5.0.0",
        "morgan": "^1.9.0",
        "nyc": "^11.4.1",
        "pug": "^2.0.0-rc.4",
        "pug-lint": "^2.5.0",
        "sinon": "^4.2.2",
        "source-map-support": "^0.5.3",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^2.6.2",
        "watch": "^1.0.2"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "app/public/scripts/*.js",
            "test/**/*",
            "coverage/**/*"
        ],
        "reporter": [
            "html",
            "text"
        ],
        "all": true
    }
}
